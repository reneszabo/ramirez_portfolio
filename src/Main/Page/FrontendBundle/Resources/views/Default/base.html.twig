<!DOCTYPE html>
<html>
  <head>
    {{ include('meta.html.twig') }}
    <title>{% block title %}Rene Ramirez - Portfolio{% endblock %}</title>
    {% block fonts %}
      <link href='https://fonts.googleapis.com/css?family=Raleway:400,100,200,300,500,600,700,800,900' rel='stylesheet' type='text/css'>
      <link rel="stylesheet" id="Open Sans-css" href="https://fonts.googleapis.com/css?family=Open+Sans%3A300%2C400%2C600%2C700%27+rel%3D%27stylesheet%27+type%3D%27text%2Fcss%27&amp;ver=4.4.2" type="text/css" media="all">
    {% endblock %}
    {% block stylesheets %}
      {{ include('css.html.twig') }}
      <link href="/css/ramirez-portfolio.css" rel="stylesheet" type="text/css"/>
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="/favicon.ico" />
  </head>
  <body>
    <a class="sr-only sr-only-focusable" href="#content">Skip to main content</a>

  {% block body %}{% endblock %}
  {% block javascripts %}
    {{ include('javascripts.html.twig') }}
    <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
    {{ include('analytics.html.twig') }}
  {% endblock %}
</body>
</html> 
